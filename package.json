    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "prettier-config-carbon": "^0.2.0",
    "standard-changelog": "^2.0.24",
    "stylelint": "^13.2.1",
