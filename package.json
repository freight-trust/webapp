{
  "name": "@freight-trust/webapp",
  "private": true,
  "version": "1.24.0",
  "license": "Apache-2.0",
  "email": "corporate@freighttrust.com",
  "url": "https://freighttrust.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/freight-trust/webapp.git"
  },
  "scripts": {
    "dev": "npm run pretest && gatsby develop -H 0.0.0.0",
    "dev:clean": "yarn clean && yarn dev",
    "build": "gatsby build",
    "build:clean": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build:analyze": "ANALYZE=true yarn build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "pretest": "./node_modules/.bin/eslint --ext .js src",
    "gh": "gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/freight-trust/webapp.git",
    "format": "prettier --write \"**/*.{css,scss,json,html,yaml,mdx}\"",
    "lint:js": "eslint . --fix",
    "lint:scss": "stylelint \"packages/**/*.scss\" --fix"
  },
  "eslintIgnore": [
    "**/node_modules/",
    "**/public/",
    "**/.cache/",
    "**/build/"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{css,scss,json,html,yaml}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "test:links:check": "find . -name \\*.md ! -path \"./node_modules/*\" -exec ./node_modules/markdown-link-check/markdown-link-check -q -c CI/linkchecker/link_check_conf.json {} \\; > linkchecker.out 2>&1",
    "test:links:display": "cat linkchecker.out",
    "test:links:verify": "! grep 'ERROR:' linkchecker.out",
    "test:links": "npm run test:links:check && npm run test:links:display && npm run test:links:verify"
  },
  "dependencies": {
    "@carbon/charts": "^0.34.8",
    "@carbon/charts-react": "^0.34.8",
    "@carbon/elements": "^10.11.0",
    "@carbon/icons-react": "^10.10.0",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "@reach/router": "^1.2.1",
    "auth0-js": "^9.13.4",
    "beautiful-react-hooks": "^0.23.1",
    "carbon-components": "^10.16.0",
    "carbon-components-react": "^7.16.0",
    "carbon-icons": "^7.0.7",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.2.0",
    "d3": "5.14.2",
    "emotion-theming": "^10.0.10",
    "eslint-plugin-import": "^2.0",
    "eslint-plugin-jsx-a11y": "^6.0",
    "eslint-plugin-mdx": "^1.7.1",
    "eslint-plugin-react-hooks": "2.0",
    "gatsby": "^2.24.14",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-feed-generator": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.3.11",
    "gatsby-plugin-google-tagmanager": "^2.3.11",
