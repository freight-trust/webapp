{
  "name": "@freight-trust/webapp",
  "private": true,
  "version": "1.8.2",
  "license": "Apache-2.0",
  "email": "corporate@freighttrust.com",
  "url": "https://freighttrust.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/freight-trust/webapp.git"
  },
  "scripts": {
    "dev": "npm run pretest && gatsby develop -H 0.0.0.0",
    "dev:clean": "yarn clean && yarn dev",
    "build": "gatsby build",
    "build:clean": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build:analyze": "ANALYZE=true yarn build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "pretest": "./node_modules/.bin/eslint --ext .js src",
    "gh": "gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/freight-trust/webapp.git",
    "format-prettier": "prettier --write \"**/*.{css,scss,json,html,yaml,mdx}\"",
    "lint": "eslint --ext js,jsx src",
    "lint:js": "eslint . --fix",
    "lint:scss": "stylelint \"packages/**/*.scss\" --fix",
    "lint:fix": "eslint --ext js,jsx src --fix",
    "format-lint": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "npm-lock": "npm i --package-lock-only",
    "prettier:diff": "prettier --check \"**/*.{scss,css,js,md}\"",
    "ci-check": "yarn prettier:diff && yarn lint && yarn test --runInBand && yarn test-ssr"
  },
  "eslintIgnore": [
    "**/node_modules/",
    "**/public/",
    "**/.cache/",
    "**/build/"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{css,scss,json,html,yaml}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "test:links:check": "find . -name \\*.md ! -path \"./node_modules/*\" -exec ./node_modules/markdown-link-check/markdown-link-check -q -c CI/linkchecker/link_check_conf.json {} \\; > linkchecker.out 2>&1",
    "test:links:display": "cat linkchecker.out",
    "test:links:verify": "! grep 'ERROR:' linkchecker.out",
    "test:links": "npm run test:links:check && npm run test:links:display && npm run test:links:verify"
  },
  "dependencies": {
    "@carbon/charts": "^0.34.8",
    "@carbon/charts-react": "^0.34.8",
    "@carbon/elements": "^10.11.0",
    "@carbon/icons-react": "^10.10.0",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "@okta/okta-signin-widget": "^4.4.4",
    "@reach/router": "^1.2.1",
    "auth0-js": "^9.13.4",
    "beautiful-react-hooks": "^0.23.1",
    "carbon-components": "^10.16.0",
    "carbon-components-react": "^7.16.0",
    "carbon-icons": "^7.0.7",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.2.0",
    "d3": "5.14.2",
    "emotion-theming": "^10.0.27",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-carbon-base": "1.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-wesbos": "1.0.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "focus-trap-react": "^8.1.0",
    "gatsby": "^2.24.50",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-feed-generator": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.3.11",
    "gatsby-plugin-google-tagmanager": "^2.3.11",
    "gatsby-plugin-iubenda-cookie-footer": "^1.1.1",
    "gatsby-plugin-lunr": "^1.5.2",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.3.2",
    "gatsby-plugin-mdx": "^1.1.2",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-netlify-cms": "^4.3.14",
    "gatsby-plugin-next-seo": "^1.6.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.0.0-beta.5",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "gatsby-remark-autolink-headers": "^2.3.11",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-remark-unwrap-images": "^1.0.1",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-theme-carbon": "^1.26.7",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-yaml": "^2.3.1",
    "lodash.throttle": "^4.1.1",
    "netlify-cms-app": "^2.12.24",
    "netlify-cms-backend-github": "^2.11.6",
    "netlify-cms-core": "^2.30.7",
    "node-sass": "^4.11.0",
    "prism-react-renderer": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^6.0.0-beta",
    "react-helmet-async": "^1.0.6",
    "react-responsive": "^8.0.3",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.3.0",
    "react-use-auth": "^0.7.5",
    "recharts": "^1.8.5",
    "remark-html": "^12.0.0",
    "remark-lint-strong-marker": "^2.0.1",
    "remark-preset-lint-consistent": "^3.0.1",
    "remark-slug": "^6.0.0",
    "slugify": "^1.3.4",
    "smooth-scroll": "^16.1.3",
    "standard-version": "^8.0.2",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^0.5.8",
    "eslint-plugin-jsdoc": "^30.5.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "ghooks": "^1.2.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "prettier-eslint": "^11.0.0",
    "react-test-renderer": "^16.13.1",
    "webpack": "^4.43.0"
  },
  "peerDependencies": {
    "react-refresh": "^0.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretest"
    }
  },
  "contributors": [
    "Freight Trust & Clearing <corporate@freighttrust.com>"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "react",
      "jsdoc",
      "jsx-a11y"
    ],
    "rules": {
      "react/jsx-uses-vars": 1,
      "react/jsx-uses-react": 1,
      "react/no-find-dom-node": 1,
      "react/no-typos": 2,
      "jsdoc/check-param-names": 2,
      "jsdoc/check-tag-names": 2,
      "jsdoc/check-types": 2,
      "jsx-a11y/no-static-element-interactions": 1,
      "jsx-a11y/no-noninteractive-element-interactions": 1,
      "jsx-a11y/click-events-have-key-events": 1,
      "jsx-a11y/anchor-is-valid": 1,
      "jsx-a11y/interactive-supports-focus": 1,
      "jsx-a11y/label-has-for": [
        1,
        {
          "components": [
            "Label"
          ],
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          },
          "allowChildren": true
        }
      ]
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true,
      "jasmine": true
    }
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run standard"
    }
  }
}
